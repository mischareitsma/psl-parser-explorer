{
  "name": "psl-parser-explorer",
  "version": "0.0.1",
  "description": "PSL Parser exploration tool",
  "main": "out/index.js",
  "scripts": {
    "build": "npm run compile && npm run copy-statics && npm run copy-js && npm run webpack",
    "clean": "rm -rf out/",
    "compile": "tsc -p .",
    "webpack": "npx webpack",
    "copy-statics": "cp -r src/{static,views} out/",
    "copy-js": "cp src/*.js out/",
    "dev": "npm run clean && npm run build && npm run web",
    "web": "node ./out/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mischareitsma/psl-parser-explorer.git"
  },
  "author": "Mischa Reitsma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mischareitsma/psl-parser-explorer/issues"
  },
  "homepage": "https://github.com/mischareitsma/psl-parser-explorer",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.1",
    "typescript": "^4.3.5",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "psl-parser": "^0.0.3",
    "pug": "^3.0.2"
  }
}
